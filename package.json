{
  "name": "hosjoy-wi-ui",
  "version": "2.2.5",
  "private": false,
  "description": "一款基于taro的ui组件库",
  "module": "dist/libsrc/index.esm.js",
  "main": "dist/libsrc/index.js",
  "source": "src/index.js",
  "templateInfo": {
    "name": "default",
    "typescript": false,
    "css": "sass"
  },
  "sideEffects": [
    "*.scss",
    "*.css"
  ],
  "keywords": [
    "wi",
    "wi-ui",
    "ui"
  ],
  "files": [
    "dist",
    "README.md"
  ],
  "scripts": {
    "build:weapp": "taro build --type weapp",
    "build:swan": "taro build --type swan",
    "build:alipay": "taro build --type alipay",
    "build:tt": "taro build --type tt",
    "build:h5": "taro build --type h5",
    "build:rn": "taro build --type rn",
    "build:qq": "taro build --type qq",
    "build:jd": "taro build --type jd",
    "build:quickapp": "taro build --type quickapp",
    "build:component": "node build/bin/version.js && cross-env TARO_BUILD_TYPE=component taro build --type ui",
    "build:publish": "cross-env TARO_BUILD_TYPE=component taro build --type weapp",
    "build:test": "cross-env TARO_BUILD_TYPE=component taro build:lib",
    "build:version": "node build/bin/release.js",
    "build:rollup": "rollup --config ./rollup.config.js",
    "dev:weapp": "npm run build:weapp -- --watch",
    "dev:swan": "npm run build:swan -- --watch",
    "dev:alipay": "npm run build:alipay -- --watch",
    "dev:tt": "npm run build:tt -- --watch",
    "dev:h5": "npm run build:h5 -- --watch",
    "dev:rn": "npm run build:rn -- --watch",
    "dev:qq": "npm run build:qq -- --watch",
    "dev:jd": "npm run build:jd -- --watch",
    "dev:quickapp": "npm run build:quickapp -- --watch"
  },
  "browserslist": [
    "last 3 versions",
    "Android >= 4.1",
    "ios >= 8"
  ],
  "author": "hosjoy",
  "license": "MIT",
  "dependencies": {
    "@babel/runtime": "^7.7.7",
    "@tarojs/cli": "3.3.0",
    "@tarojs/components": "3.3.0",
    "@tarojs/react": "3.3.0",
    "@tarojs/runtime": "3.3.0",
    "@tarojs/taro": "3.3.0",
    "lodash": "4.17.15",
    "react": "^17.0.0",
    "react-dom": "^17.0.0"
  },
  "devDependencies": {
    "@babel/core": "^7.8.0",
    "@babel/plugin-transform-modules-commonjs": "^7.14.0",
    "@rollup/plugin-commonjs": "^11.0.2",
    "@rollup/plugin-json": "^4.0.2",
    "@rollup/plugin-node-resolve": "^7.1.1",
    "@tarojs/mini-runner": "3.3.0",
    "@tarojs/webpack-runner": "3.3.0",
    "@types/react": "^17.0.2",
    "@types/webpack-env": "^1.13.6",
    "babel-preset-taro": "3.3.0",
    "cross-env": "^7.0.3",
    "eslint": "^6.8.0",
    "eslint-config-taro": "3.3.0",
    "eslint-plugin-import": "^2.12.0",
    "eslint-plugin-react": "^7.8.2",
    "eslint-plugin-react-hooks": "^4.2.0",
    "rollup": "^2.3.1",
    "rollup-plugin-babel": "^4.4.0",
    "rollup-plugin-copy": "^3.3.0",
    "rollup-plugin-sass": "^1.2.2",
    "rollup-plugin-typescript2": "^0.27.0",
    "rollup-plugin-visualizer": "^4.0.2",
    "stylelint": "9.3.0"
  }
}
